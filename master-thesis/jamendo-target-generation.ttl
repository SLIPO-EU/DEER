:jamendo
  a deer:FileModelReader ;
  deer:fromPath "./data/jamendo_input.ttl" ;
.

:ner
  a deer:NEREnrichmentOperator ;
  fcage:hasInput ( :jamendo ) ;
  deer:literalProperty rdfs:comment ;
  deer:importProperty foxo:relatedTo ;
  deer:parallelism 10 ;
  deer:neType "all" ;
.

:write_ner_output2
  a deer:FileModelWriter ;
  fcage:hasInput ( :jamendo ) ;
  deer:outputFile "./data/jamendo_test.nt" ;
  deer:outputFormat "NT" ;
.

:write_ner_output
  a deer:FileModelWriter ;
  fcage:hasInput ( :ner ) ;
  deer:outputFile "./data/jamendo_train.nt" ;
  deer:outputFormat "NT" ;
.
